{
    "rewrites": [
      { "source": "/(.*)", "destination": "/chainrate-nextjs/$1" }
    ],
    "buildCommand": "cd chainrate-nextjs && npm run build",
    "outputDirectory": "chainrate-nextjs/.next",
    "installCommand": "cd chainrate-nextjs && npm install",
    "builds": [
      {
        "src": "chainrate-nextjs/next.config.js",
        "use": "@vercel/next"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "chainrate-nextjs/$1"
      }
    ],
    "env": {
      "NODE_ENV": "production"
    }
  }